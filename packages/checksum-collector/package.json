{
  "name": "@pkgdeps/checksum-collector",
  "version": "1.1.1",
  "description": "Collection checking checksum operations in shellscript.",
  "keywords": [
    "shellscript",
    "parser",
    "checksum"
  ],
  "homepage": "https://github.com/pkgdeps/unverified-checksum-checker/tree/master/packages/checksum-collector/",
  "bugs": {
    "url": "https://github.com/pkgdeps/unverified-checksum-checker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pkgdeps/unverified-checksum-checker.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "main": "lib/checksum-collector.js",
  "module": "module/checksum-collector.js",
  "types": "lib/checksum-collector.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "build": "tsc -p . && tsc -p ./tsconfig.module.json",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/ module/",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/structured-source": "^3.0.0",
    "mocha": "^9.1.1",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/minimist": "^1.2.2",
    "minimist": "^1.2.5",
    "mvdan-sh": "^0.5.0",
    "shlex": "^2.1.0",
    "structured-source": "^3.0.2"
  }
}
