{
  "name": "@pkgdeps/secretlint-rule-checksum",
  "version": "1.0.0",
  "description": "secretlint rule that check if checking checksum.",
  "keywords": [
    "secretlint",
    "checksum",
    "lint",
    "shellscript"
  ],
  "homepage": "https://github.com/pkgdeps/unverified-checksum-checker/tree/master/packages/secretlint-rule-checksum/",
  "bugs": {
    "url": "https://github.com/pkgdeps/unverified-checksum-checker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pkgdeps/unverified-checksum-checker.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "main": "lib/secretlint-rule-checksum.js",
  "module": "module/secretlint-rule-checksum.js",
  "types": "lib/secretlint-rule-checksum.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "build": "tsc -p . && tsc -p ./tsconfig.module.json",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/ module/",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "@pkgdeps/checksum-collector": "^1.0.0"
  },
  "devDependencies": {
    "@secretlint/tester": "^3.3.0",
    "@secretlint/types": "^3.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "mocha": "^9.1.1",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
